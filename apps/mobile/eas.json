{
  "cli": {
    "version": ">= 7.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "image": "latest",
        "credentialsSource": "remote"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_USE_FAST_RESOLVER": "1"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "ios": {
        "autoIncrement": true,
        "buildConfiguration": "Release"
      },
      "android": {
        "autoIncrement": true,
        "buildType": "app-bundle"
      }
    }
  }
}
